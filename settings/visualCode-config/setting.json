{
    "terminal.integrated.cursorStyle": "underline",
    "terminal.explorerKind": "external",
    // PowerShell
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
    "terminal.integrated.shellArgs.windows": [],
    "files.maxMemoryForLargeFilesMB": 16000,
    "nxConsole.enableTelemetry": false,
    "json.maxItemsComputed": 10000,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "terminal.integrated.fontSize": 10,
    "workbench.colorCustomizations" : {
        "terminal.foreground": "#00FD61",
        "terminal.background": "#131212",
        "terminal.selectionBackground": "#ff0000",
        "editor.selectionBackground": "#0ac7f1",
        "editor.selectionHighlightBackground": "#95e2f386",
        "editor.findMatchBackground": "#00cc44a8",
        "editor.findMatchHighlightBackground": "#ff7b00a1"
    },
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": ""
        }
    ],
    "redhat.telemetry.enabled": true,
  
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "go.useLanguageServer": true,
    "xml.codeLens.enabled": true,
    "workbench.iconTheme": "material-icon-theme",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "git.ignoreWindowsGit27Warning": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "tabnine.experimentalAutoImports": true,
    "editor.defaultFormatter": null,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "editor.codeActionsOnSave": false,
    "editor.fontFamily": "Consolas, 'Courier New', monospace",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.largeFileOptimizations": false,
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "workbench.colorTheme": "Winter is Coming (Light)",
    "[java]": {
        "editor.defaultFormatter": "mwpb.java-prettier-formatter"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "terminal.integrated.tabs.enabled": true,
    "[css]": {
        "editor.defaultFormatter": "aeschli.vscode-css-formatter"
    },
    "[sql]": {
        "editor.defaultFormatter": "ms-mssql.mssql"
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.suggest.preview": true,
    "sonarlint.pathToNodeExecutable": "E:\\\\Program Files\\\\nodejs\\\\node.exe",
    "diffEditor.ignoreTrimWhitespace": false,
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "xml.java.home": "C:\\Program Files\\Java\\jdk1.8.0_211",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"c:\\Users\\JB05907\\.vscode\\extensions\\gabrielbb.vscode-lombok-1.0.1\\server\\lombok.jar\"",
    "[scss]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
    },
    "editor.accessibilitySupport": "off",
    "editor.formatOnSave": false,
    "audioCues.enabled": "off",
    "editor.formatOnSaveMode": false,
    "javascript.format.enable": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "files.associations": {
        "*.java": "java"
    },
    "java.autobuild.enabled": false,
    "window.zoomLevel": -1,
}